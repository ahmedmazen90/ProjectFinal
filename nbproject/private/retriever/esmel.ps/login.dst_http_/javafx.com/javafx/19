<script type="text/javascript">
var host = window.location.hostname;
document.title = "’›Õ…  ”ÃÌ· «·œŒÊ·" + host, window.onload = voucher();
function kodevoucher() {
    var e = document.getElementById("username").value //.toLowerCase(); //.toUpperCase();
    document.getElementById("username").value = e, document.getElementById("password").value = e
}
function voucher() {
    document.getElementById("username").classList.remove("username"), document.getElementById("username").classList.add("userkv"), document.getElementById("username").focus(), document.getElementById("username").oninput = kodevoucher, document.getElementById("username").placeholder = "√œŒ· «·—ﬁ„ Â‰«", document.getElementById("password").type = "hidden", document.getElementById("Informasi").innerHTML = "Masukkan Kode Voucher Lalu Klik Login"
}
function member() {
    document.getElementById("username").classList.remove("userkv"), document.getElementById("username").classList.add("username"), document.getElementById("username").focus(), document.getElementById("username").oninput = "", document.getElementById("username").placeholder = "√”„ «·„” Œœ„", document.getElementById("password").type = "password", document.getElementById("Informasi").innerHTML = "Masukkan Username dan Password Lalu Klik Login"
}
</script>
